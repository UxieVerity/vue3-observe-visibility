{
  "name": "vue3-observe-visibility",
  "version": "1.0.0",
  "description": "Detect when an element is becoming visible or hidden on the page. ",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "unpkg": "dist/index.browser.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:es && npm run build:umd && npm run build:browser",
    "build:es": "rollup -c build/rollup.config.es.js",
    "build:umd": "rollup -c build/rollup.config.umd.js",
    "build:browser": "rollup -c build/rollup.config.browser.js"
  },
  "keywords": [
    "vue3"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UxieVerity/vue3-observe-visibility.git"
  },
