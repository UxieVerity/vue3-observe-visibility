  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UxieVerity/vue3-observe-visibility/issues"
  },
  "homepage": "https://github.com/UxieVerity/vue3-observe-visibility#readme",
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/node": "^20.1.7",
    "rollup": "^3.22.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
